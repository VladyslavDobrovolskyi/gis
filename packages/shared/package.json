{
  "name": "@gis/shared",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -b",
    "lint": "eslint --ext .ts",
    "lint:fix": "eslint --ext .ts  --fix",
    "prettier:check": "pnpm prettier --check **/*.{ts,js,json,html,css,yaml,yml}",
    "prettier:write": "pnpm prettier --write **/*.{ts,js,json,html,css,yaml,yml} --write"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "default": "./dist/schemas/index.js"
    },
    "./openapi/*": {
      "types": "./dist/openapi/index.d.ts",
      "default": "./dist/openapi/index.js"
    }
  },
  "dependencies": {
    "zod": "^4.0.0",
    "zod-openapi": "^5.4.5"
  }
}
